{
  "name": "meinauto-js",
  "version": "0.1.1",
  "description": "Class autoload and dependency manager",
  "main": "./src/lib/init/Run.js",
  "scripts": {
    "generate-docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "generate-styles": "node_modules/.bin/node-sass --recursive --output web/css --source-map true src/lib/ui",
    "watch-styles": "node_modules/.bin/node-sass --watch --recursive --output web/css --source-map true src/lib/ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeroxzone/meinauto-js.git"
  },
  "dependencies": {
    "jquery": "~3.2.1"
  },
  "devDependencies": {
    "node-sass": "~4.5.3",
    "qunitjs": "~2.3.3",
    "jsdoc": "~3.4.3",
    "ink-docstrap": "~1.3.0"
  },
  "keywords": [
    "meinauto"
  ],
  "author": "Ralph Zickert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xeroxzone/meinauto-js/issues"
  },
  "homepage": "https://github.com/xeroxzone/meinauto-js#readme",
  "jshintConfig": {
    "maxerr": 50,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "immed": false,
    "latedef": false,
    "newcap": false,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": false,
    "undef": true,
    "unused": true,
    "strict": true,
    "maxparams": false,
    "maxdepth": false,
    "maxstatements": false,
    "maxcomplexity": false,
    "maxlen": false,
    "varstmt": false,
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": false,
    "esversion": 5,
    "moz": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": true,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": false,
    "multistr": false,
    "noyield": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "shadow": false,
    "sub": false,
    "supernew": true,
    "validthis": false,
    "browser": true,
    "browserify": false,
    "couch": false,
    "devel": true,
    "dojo": false,
    "jasmine": false,
    "jquery": true,
    "mocha": false,
    "mootools": false,
    "node": false,
    "nonstandard": false,
    "phantom": false,
    "prototypejs": false,
    "qunit": true,
    "rhino": false,
    "shelljs": false,
    "typed": false,
    "worker": false,
    "wsh": false,
    "yui": false,
    "globals": {
      "MeinAutoJs": true
    }
  }
}
